<Application x:Class="ThinkVoip.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ThinkVoip" xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <materialDesign:BundledTheme x:Name="Theme" BaseTheme="Light"  PrimaryColor="LightGreen" SecondaryColor="LightBlue" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <!--<FontFamily x:Key="JetBrainsMono-Regular">pack://application:,,,/Fonts/#Jetbrains Mono</FontFamily>-->
            <!--<FontFamily x:Key="JetBrainsMono-Bold">pack://application:,,,/Fonts/#Jetbrains Mono-Bold</FontFamily>-->
        </ResourceDictionary>
    </Application.Resources>
</Application>
